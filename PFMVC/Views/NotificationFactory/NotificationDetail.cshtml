@{
    ViewBag.Title = "NotificationDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<fieldset>
    <legend>Notification Detail...</legend>
    <ul>
        @for (int i = 0; i < ViewBag.NotificationDetail.Count; i++)
        {
            <li>@ViewBag.NotificationDetail[i] <a href="@ViewBag.Link[i]">Click Here...</a></li>
        }

    </ul>
    

</fieldset>
